{
  "name": "NewNewTwitter Swap panels",
  "version": "0.1.1",
  "description": "Swaps dashboard and content blocks for #NewNewTwitter interface.",
 "content_scripts" : [
    {
      "matches" : [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "js" : ["contentscript.js"],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ],
  "permissions": [
    "http://twitter.com/",
    "https://twitter.com/"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  }
}
